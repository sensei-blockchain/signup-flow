{
  "name": "test-task",
  "version": "1.0.0",
  "description": "Signup flow",
  "main": "server.js",
  "scripts": {
    "precommit": "npm run lint",
    "build": "npm run clean & NODE_ENV=development node_modules/.bin/webpack -p",
    "build-production": "npm run clean && NODE_ENV=production node_modules/.bin/webpack",
    "build-production-ssr": "node_modules/.bin/webpack --config webpack.config.server.js",
    "build-serve-static": "npm run build-production && npm run serve-static",
    "build-serve-ssr": "npm run build-production && npm run build-production-ssr && npm run serve-ssr",
    "serve-static": "node staticFileServer.js",
    "serve-ssr": "node index.js",
    "clean": "rm -rf compiled/ dist/",
    "dev": "NODE_ENV=development node_modules/.bin/webpack-dev-server -d --port 3000 --inline --hot --progress --no-info",
    "heroku-postbuild": "npm run build-production",
    "lint": "node_modules/.bin/eslint --ext .jsx --ext .js src/ --ignore-pattern *.spec.jsx",
    "test": "mocha --require ignore-styles -w src/test/setup.js \"src/{**,}/*spec.js*\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
